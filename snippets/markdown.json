{
  "Readme": {
    "prefix": "readme",
    "body": [
      "# ${1: Project Title}\n",
      "Author(s): \n",
      "Date: \n",
      "## Motivation\n",
      "## Project Execution\n",
      "## Known Bugs\n",
      "## Acknowledgements\n"
    ],
    "description": "README template"
  },
  "Readme w/ Pictures": {
    "prefix": "readme",
    "body": [
      "# ${1: Project Title}\n",
      "Author(s): \n",
      "Date: \n",
      "## Motivation\n",
      "## Project Execution\n",
      "## Project Execution\n",
      "## Known Bugs\n",
      "## Pictures\n\n",
      "## Acknowledgements\n"
    ],
    "description": "README template with Picture"
  },
  "Metadata Front Matter": {
    "prefix": "meta",
    "body": [
      "---",
      "path: $1",
      "title: $2",
      "date: $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE",
      "excerpt: ",
      "author: ",
      "tags: []",
      "---\n"
    ],
    "description": "Markdown metadata front matter"
  },
  "Note File": {
    "prefix": "note",
    "body": [
      "---",
      "title: $1",
      "date: $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE",
      "author: $USER",
      "---\n",
      "\n**Tags**: $0"
    ],
    "description": "Template for a quick note file"
  }
}
